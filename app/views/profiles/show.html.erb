<div class="container">
  <div class="row">
    <div class="col-md-10 offset-md-1"><h1 class="text-4xl font mt-4">プロフィール詳細</h1>
      <%= @profile.dinner_time.strftime( "%H:%M" ) %>
      <%= @profile.bedtime.strftime( "%H:%M" ) %>
      <% @profile.day_of_weeks.each do |day| %>
        <%= day.name %>
      <% end %>
    </div>
  </div>
</div>
